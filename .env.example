# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=symm_blocks
DB_USER=postgres
DB_PASSWORD=your_password

# Primary Account (This is the account that will own the moderation list)
PRIMARY_BLUESKY_HANDLE=your-primary-handle.bsky.social
PRIMARY_BLUESKY_PASSWORD=your-primary-password

# Secondary Accounts (Add as many as needed)
# Format: handle1,password1;handle2,password2 or handle1:password1;handle2:password2
# Each account is separated by semicolon (;)
# Handle and password are separated by either comma (,) or colon (:)
SECONDARY_ACCOUNTS=account1.bsky.social,password1;account2.bsky.social,password2

# API Configuration
CLEARSKY_API_URL=https://api.bsky.app
BLUESKY_API_URL=https://bsky.social
POLLING_INTERVAL=300  # seconds between checks for new blocks

# Moderation List Configuration
MOD_LIST_NAME=symmetrical blocks
MOD_LIST_PURPOSE=Automatically synchronized blocks across multiple accounts
MOD_LIST_DESCRIPTION=This list contains accounts that are blocked by or blocking any of my accounts.

# Logging
LOG_LEVEL=INFO 