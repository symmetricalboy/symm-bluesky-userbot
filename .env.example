# Production DB settings (used when LOCAL_TEST=false)
DB_HOST="${{Postgres.PGHOST}}"
DB_PORT="${{Postgres.PGPORT}}"
DB_NAME="${{Postgres.PGDATABASE}}"
DB_USER="${{Postgres.PGUSER}}"
DB_PASSWORD="${{Postgres.PGPASSWORD}}"

# Test mode settings
LOCAL_TEST="true"
TEST_DATABASE_URL="postgresql://username:password@host:port/database"
TEST_PRIMARY_BLUESKY_HANDLE="your-test-account.bsky.social"
TEST_PRIMARY_BLUESKY_PASSWORD="your-test-password"
TEST_SECONDARY_ACCOUNTS="account1.bsky.social,password1"
TEST_VALID_DID="did:plc:your-test-did"
TEST_PAGINATION_BLUESKY_HANDLE="your-pagination-test.bsky.social"
TEST_PAGINATION_BLUESKY_PASSWORD="your-pagination-password"

# Production account credentials
PRIMARY_BLUESKY_HANDLE="your-primary-account.bsky.social"
PRIMARY_BLUESKY_PASSWORD="your-primary-password"
SECONDARY_ACCOUNTS="account1.bsky.social,password1;account2.bsky.social,password2;account3.bsky.social,password3"

# API endpoints
CLEARSKY_API_URL="https://api.clearsky.services/api/v1/anon"
BLUESKY_API_URL="https://bsky.social"

# Application settings
POLLING_INTERVAL="60"
MOD_LIST_NAME="your moderation list name"
MOD_LIST_PURPOSE="Your moderation list purpose"
MOD_LIST_DESCRIPTION="Your moderation list description"
LOG_LEVEL="INFO"
SYNC_INTERVAL_PRIMARY_MINUTES="1440"
SYNC_INTERVAL_SECONDARY_MINUTES="1440"

# Jetstream settings
FIREHOSE_HOST="jetstream.atproto.tools"
FIREHOSE_PORT="443"
FIREHOSE_SSL="true" 